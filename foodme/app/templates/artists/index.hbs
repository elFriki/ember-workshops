<div class="col-md-3">

		<ul class="list-group">
			{{#if cienXcien}}
			<li class="list-group-item active">
				<span class="badge">
					<i class="glyphicon glyphicon-filter"></i>
				</span>
		    {{filteredArtists.length}} {{t "artists"}}
		  </li>
			{{else}}
		  <li class="list-group-item">
				<span class="badge">
					<i class="glyphicon glyphicon-filter"></i>
				</span>
				<span class="badge btn">
					<i class="glyphicon glyphicon-remove" {{action 'clear'}}></i>
				</span>
		    <strong>{{filteredArtists.length}} {{t "artists"}}</strong>
		  </li>
			{{/if}}
			<li class="list-group-item">
				<span class="badge">
					<i class="glyphicon glyphicon-sort-by-alphabet-alt"></i>
				</span>
				{{#if filterName}}
					<span class="badge btn">
					<i class="glyphicon glyphicon-remove" {{action 'clearName'}}></i>
					</span>
				{{/if}}
				<div class="input-group">
					{{input value=filterName placeholder=(t 'name') class="form-control" aria-describedby="nameFilter"}}
				</div>
		  </li>
